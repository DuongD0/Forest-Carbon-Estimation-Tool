# Production Frontend Configuration
# API Configuration
REACT_APP_API_BASE_URL=https://api.your-domain.com/api/v1

# Auth0 Configuration - REPLACE WITH YOUR AUTH0 SETTINGS
REACT_APP_AUTH0_DOMAIN=your-domain.auth0.com
REACT_APP_AUTH0_CLIENT_ID=your_auth0_client_id
REACT_APP_AUTH0_AUDIENCE=https://your-api-audience

# Production Mode
REACT_APP_DEV_MODE=false
REACT_APP_ENVIRONMENT=production

# Analytics (optional)
REACT_APP_GOOGLE_ANALYTICS_ID=GA-XXXXXXXXX

# Feature Flags
REACT_APP_ENABLE_P2P_MARKETPLACE=true
REACT_APP_ENABLE_ADVANCED_ANALYTICS=true

# Build Configuration
GENERATE_SOURCEMAP=false
